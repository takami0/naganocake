<p>配送先登録/一覧</p>

<%= form_with model: @address, url: public_addresses_path, method: :post do |f| %>
  <%= f.label "郵便番号（ハイフンなし）"%>
  <%= f.text_field :postal_code%><br>
  
  <%= f.label "住所"%>
  <%= f.text_field :address%><br>
  
  <%= f.label "宛名"%>
  <%= f.text_field :name%>
  
  <%= f.submit "新規登録"%>
<%end%>

<table>
  <tr>
    <th>郵便番号</th>
    <th>住所</th>
    <th>宛名</th>
    <th></th>
  </tr>
  <tr>
    <td></td>
    <td></td>
    <td></td>
    
  </tr>
</table>